		"tests": {
			"description": "Build tests",
			"dependencies": ["gtest"]
		}
	},
	"dependencies": ["nlohmann-json"],
	"overrides": [
        { "name": "cxxopts", "version": "3.1.1" },
        { "name": "nlohmann-json", "version": "3.11.2" },
        { "name": "gtest", "version": "1.13.0" }
    ]
}
